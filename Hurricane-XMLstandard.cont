			<patch1>
				<region>region0</region>
				<patchVar>
					<![CDATA[p,v]]>
				</patchVar>
				<p>1.0e5</p>
				<v>(100.0,20.0,0.0)</v>				<!-- Dimensional parameters-->
			</patch1>

	</initialization>


	<!--[7] Write Setting-->
	<writeControl>
		<solutionWrite>
			<writeType>tecplot</writeType>			<!--tecplot, cgns-->
			<solutionType>onlyInstantaneous</solutionType>
			<!-- onlyInstantaneous,
			onlyAverage,
			onlyPulse,
			InatsntAndAve,
			InstantAndAveAndPulse.
			Default is onlyInstantaneous. 
			-->
			<varList>
				<![CDATA[
				rho,v,p,E,T,TGrad,Ma,pt,pt_constCp,Tt,Tt_constCp,viscousRatio,mu,mut,kt,wt,cp,gamma,kappa,GFO,allSpecies,allSpecies-mole,
				heatReleaseRate,Damkohler,KolmogorovLengthScale,KolmogorovTimeScale,KolmogorovVelocityScale,flameIndex,Shadowgraph,
				integralLengthScale,integralTimeScale,integralVelocityScale,turbulentKineticEnergy,turbulentDissipationRate,
				tcFRR,tcJacDT,tcSFR,tcGSPR,hat,cellVolume,QCriterion,OmegaCriterion,DeltaCriterion,vorticity,
				mixtureFractionZ,mixtureDisspationRate,PaSRKappa,PaSRTauch,PaSRTaumix
				]]>  
			</varList>

			<tcGSPR>
				<![CDATA[H2O,CO2]]>
			</tcGSPR>
			<startTime>1e-4</startTime>
			<sampling>
				<isSampling>on</isSampling>				<!--Default is off.-->
				<samplingStep>10</samplingStep>				<!--Default is 10.-->
			</sampling>

			<writeProfile>
				<profileType>Fluent</profileType>				<!-- Fluent or OpenHurricane. Default is OpenHurricane. -->
				<fileName>HurricaneProfile.dat</fileName>
				<profileList>
					<![CDATA[
						profile1,profile_inlet
					]]>
				</profileList>
				<profile1>
					<profileFace>outlet</profileFace>
					<fieldNameList>
						<![CDATA[
							v,ReynoldsStressTensor
						]]>
					</fieldNameList>
				</profile1>
				<profile_inlet>
					<profileFace>outlet</profileFace>
					<fieldNameList>
						<![CDATA[
							v,ReynoldsStressTensor
						]]>
					</fieldNameList>
				</profile_inlet>
			</writeProfile>
		</solutionWrite>
		
		<!--[7.2] For specific boundary face-->
			<faceZones>
				<![CDATA[faceName1,wallFaceName]]>
			</faceZones>
			<startTime>1e-4</startTime>
			<sampling>
				<isSampling>on</isSampling>			<!--Default is off.-->
				<samplingStep>10</samplingStep>			<!--Default is 10.-->
			</sampling>
			<faceName1>
				<!-- For ordinary faceZone variable array. -->
				<![CDATA[rho,v,p,T,Ma,pt,pt_constCp,Tt,Tt_constCp,viscousRatio,mu,mut,kt,wt,nut,vorticity,QVorticity,DeltaVorticity]]>
			</faceName1>

			<wallFaceName>
				<!-- For wall faceZone variable array. -->
				<![CDATA[
						rho,v,p,T,Ma,pt,Tt,
						cf,cpAbs,cpRel,Qw,YPlus,uPlus,u0,v0,w0
					]]>
			</wallFaceName>

	</writeControl>

	<testProcessTime>
		<fileName>testProcessTime.dat</fileName>
	</testProcessTime>

	<sourceTerms>
		<sourceList>
			<![CDATA[spongeRegion,gravityTerms]]>
		</sourceList>

		<!-- As an example for setting sponge layer for damping waves: -->
		<spongeRegion>
			<!-- For sponge region, there can only be one mark region. -->
			<regions>
				<![CDATA[region0]]>
			</regions>
			<sourceType>spongeRegion</sourceType>			<!-- spongeRegion, gravityForce -->
			<sigma0>20</sigma0>			<!-- The maximum sponge layer profile value. Default is 20. -->
			<m>2</m>			<!-- The integer parameter controlling the shape of the profile. Default is 2. -->
			<faceZoneName>FAR</faceZoneName>			<!-- The name of the boundary face zone that connected with the sponge layer. It must be pressure-far-field boundary.-->
			<Delta>10</Delta>			<!-- The sponge layer width. Must consistent with the mark region. -->
		</spongeRegion>

		<!-- As an example for setting the gravitational body force. -->
		<gravityTerms>
			<sourceType>gravityForce</sourceType>			<!-- spongeRegion, gravityForce -->
			<gravity>(0,-9.80665,0)</gravity>			<!-- Default is (0,-9.80665,0). -->
		</gravityTerms>

	</sourceTerms>

</Config>
