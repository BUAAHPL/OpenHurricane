*****************************************************************************************
-->
<Config>
	<!--[0] Grid Setting-->
	<meshSet>
		<!--[0.1] Mesh Source-->
		<meshFile>fluent.msh</meshFile>		<!-- fluent format : .msh; OpenHurricane format : .h5-->
		<meshFormat>fluent</meshFormat>		<!-- fluent, CGNS, or OpenHurricane-->
		<unit>mm</unit>
		<reordering>RCMReordering</reordering>		<!-- MetisReordering or RCMReordering -->


		<!--[0.2] Scale Mesh with given 3D factor. -->
		<!-- <scaleMesh>(1.0,1.0,1.0)</scaleMesh> -->

		<!--[0.3] Region Section-->
		<regions>			<!--Regions list-->
			<![CDATA[shapeRegion,hex,cylinder,plane]]>
		</regions>		<!-- shape: hex, sphere or cylinder or plane-->
						<!-- Unit: m-->
		<hex>
			<id>0</id>
			<option>inside</option>
			<shape>hex</shape>
			<xmax>0.0</xmax>
			<xmin>0.0</xmin>
			<ymax>0.0</ymax>
			<ymin>0.0</ymin>
			<zmax>0.0</zmax>
			<zmin>0.0</zmin>
		</hex>
		<shapeRegion>
			<id>1</id>
			<option>inside</option>
			<shape>sphere</shape>
			<center>(0.0,0.0,0.0)</center>
			<radius>0.0</radius>
		</shapeRegion>
		<cylinder>
			<id>2</id>
			<option>inside</option>
			<shape>cylinder</shape>
			<Amax>(0.0,0.0,0.0)</Amax>
			<Amin>(0.0,0.0,0.0)</Amin>
			<radius>0.0</radius>
		</cylinder>
		<plane>
			<id>3</id>
			<option>inside</option>
			<shape>plane</shape>
			<normal>(1.0,0.0,0.0)</normal>
			<point>(0.0,0.0,0.0)</point>
		</plane>
	</meshSet>

	<!--[1] Iteration Setting-->
	<iteration>
		<!--[1.1] Step&Write setting-->
		<maxStep>200000</maxStep>
		<writeOutputStep>1</writeOutputStep>
		<!--[1.2] Relay if "restartFrom" is turned on. -->
		<!--<restartFrom>OpenHurricane-1.h5</restartFrom>-->
		<relayType>originData</relayType>		<!-- originData or interpolationData. Default is originData-->
		<nearNodes>1</nearNodes>		<!--Default is 1.-->

		<!--[1.3] Time dependence-->
		<flowState>steady</flowState>		<!-- The flow state can be steady or unsteady. -->

		<!--[1.4] Pseudo time-step method. -->
		<pseudoTime>
			<timeStepMethod>cellBasedMethod</timeStepMethod>			<!-- faceBasedMethod or cellBasedMethod -->
			<CFLSetting>
				<cflMin>1.1</cflMin>				<!-- The minimum CFL number. Default is 0.1 . -->
				<cflMax>10.0</cflMax>				<!-- The maximum CFL number. Default is 10.0 . -->
				
				<stepForPrintCFL>20</stepForPrintCFL>

				<!-- Dynamic CFL number method: -->
				<CFLType>linearCFL</CFLType>				<!-- The CFL type for adapting CFL number. 
																 Can be: linearCFL, expCFL or expertSystemAdaptCFL.
																 If not specified, the linearCFL will be used. 
															-->
				<!--(1) For "linearCFL" (linear method). -->
				<linearCFL>
					<cflConst>200</cflConst>
					<cflLinear>400</cflLinear>
				</linearCFL>

				<!-- (2) For "expCFL" (exponent method). -->
				<expCFL>
					<cflConst>200</cflConst>
				</expCFL>

				<!-- (3) For expertSystemAdaptCFL. -->
				<expertSystemAdaptCFL>
					<CFLFactor0>0.01</CFLFactor0>
					<breakdownFactor>0.5</breakdownFactor>
					<divergenceFactor>0.8</divergenceFactor>
					<residualJumpThreshold>0.5</residualJumpThreshold>
					<updateFactor>2.0</updateFactor>
					<interval>10</interval>
				</expertSystemAdaptCFL>
			</CFLSetting>
			
			<timeStepType>localTimeStep</timeStepType>			<!-- localTimeStep or globalTimeStep, the default value is localTimeStep-->
			<CForTimeStep>2.0</CForTimeStep>			<!-- Range [1,4]-->

			<isStretchAc>on</isStretchAc>				<!-- Convergence Acceleration for Stretched Meshes.
															 Notice : Only works in implicit time scheme. Default is on. -->
			<StretchAc>
				<minStretchScale>1.0</minStretchScale>
				<cflRatioMax>5000000.0</cflRatioMax>
			</StretchAc>

			<minCellOrthogonality>0.7</minCellOrthogonality>			<!-- The maximum cell orthogonality for correct the time step. Default is 0.7. -->
		</pseudoTime>

		<!--[1.5] Time scheme. -->
		<timeMethod>
			<!-- (1) LUSGS. Implicit scheme. -->
			<timeMarching>LUSGS</timeMarching>
			<!-- For unsteady flow:	<timeMarching>unsteadyLUSGS</timeMarching> -->
			<LUSGS>
				<omegaForLUSGS>1.050</omegaForLUSGS>				<!-- Range [1,2]-->
				<betaT>0.2</betaT>
			</LUSGS>

			<!-- (2) Multi-Stage Runge-Kutta. Explicit scheme. -->
			<!--<timeMarching>MRK</timeMarching>-->
			<!-- For unsteady flow:	<timeMarching>unsteadyMRK</timeMarching> -->
			<MRK>
				<MRKStage>s5</MRKStage>				<!-- Stage : f3, f4, f5, s3, s4, s5 (f = first order; s= second order)-->
				<impResSmooth>on</impResSmooth>				<!-- on or off. Implicit residual smoothing for Multi-Stage Runge-Kutta method.-->
				<ep>0.5</ep>				<!-- Range [0.3,0.8]-->
				<maxJacStep>2</maxJacStep>				<!-- The maximum Jacobi iterations. Usually two is sufficient.-->
				<cflRatioForImpResSmoo>2.0</cflRatioForImpResSmoo>				<!-- The ratio to increase the CFL number due to implicit residual smoothing. Usually 2~5 times.-->
				<cfl>1.0</cfl>
			</MRK>

			<!-- (3) Semi-Implicit Multi-Stage Runge-Kutta. Explicit scheme. -->
			<!--<timeMarching>semiImplicitMRK</timeMarching>-->
			<!-- For unsteady flow:	<timeMarching>unsteadySemiImplicitMRK</timeMarching> -->
			<semiImplicitMRK>
				<MRKStage>s5</MRKStage>				<!-- Stage : f3, f4, f5, s3, s4, s5 (f = first order; s= second order)-->
				<impResSmooth>off</impResSmooth>				<!-- on of off. Implicit residual smoothing for Multi-Stage Runge-Kutta method.-->
				<ep>0.5</ep>				<!-- Range [0.3,0.8]-->
				<maxJacStep>2</maxJacStep>				<!-- The maximum Jacobi iterations. Usually two is sufficient.-->
				<cflRatioForImpResSmoo>2.0</cflRatioForImpResSmoo>				<!-- The ratio to increase the CFL number due to implicit residual smoothing. Usually 2~5 times.-->
				<cfl>1.0</cfl>
			</semiImplicitMRK>
			
			<!-- (4) TVD Runge-Kutta-->
			<!--<timeMarching>TVDRK</timeMarching>-->
			<!-- For unsteady flow: unsteadyTVDRK -->

			<TVDRK>
				<TVDRKOrder>TVDRK2</TVDRKOrder>				<!-- TVDRK2 - 2nd order;
																 TVDRK3 - 3rd order.
															-->
				<cfl>1.0</cfl>
			</TVDRK>

			<!-- (5) 4th order TVD Runge-Kutta-->
			<!--<timeMarching>TVDRK4</timeMarching>-->
			<!-- For unsteady flow: unsteadyTVDRK4 -->
			<TVDRK4>
				<cfl>1.0</cfl>
			</TVDRK4>

			<!-- For unsteady flow. -->
			<physicalTimes>
				<dynamicTimeStep>on</dynamicTimeStep>				<!-- on or off. Default is off.-->
				<dynamicCFL>0.8</dynamicCFL>
				<!-- If the dynamicTimeStep is used, then user can specify the CFL number. If it is not given, 
				                                    it will be 0.8 as defaul for BDF123LUSGS, or taken from that of explicit scheme, such as unsteadyTVDRK, unsteadyTVDRK4 or unsteadyMRK. -->
				<phyTimeStep>1e-7</phyTimeStep>				<!-- When dynamic time-step is used, this parameter will be ignored. -->
				<maxPhyTime>0.1</maxPhyTime>
				<totalPhyTime>0.0</totalPhyTime>
			</physicalTimes>
			<dualTimeStep>
				<subIteration>
					<maxSubStep>30</maxSubStep>
					<minSubStep>20</minSubStep>
				</subIteration>
				<iterationMethod>Newton</iterationMethod>				<!-- Newton or pseudoTimeStepping-->
				<newTimeStepInitialize>LagrangeExterpolation</newTimeStepInitialize>				<!-- lastTime or LagrangeExterpolation-->
				<beginStep>2</beginStep>
			</dualTimeStep>

			<!-- (6) BDF. It is a dual time-step method. -->
			<!--<timeMarching>BDF123LUSGS</timeMarching>-->
			<BDF123LUSGS>
				<BDF123>BDF2</BDF123>				<!-- BDF1ã€BDF2 or BDF3-->
				<useLinearDtCFL>on</useLinearDtCFL>				<!-- If on, it is to use a linear dt or CFL for the initial stage of calculation. Default is off. -->
				<linearDtCFL>
					<dtCFLConst>10</dtCFLConst>
					<dtCFLLinear>20</dtCFLLinear>
				</linearDtCFL>
			</BDF123LUSGS>

			<!--<timeMarching>ESDIRKLUSGS</timeMarching>-->
			<ESDIRKLUSGS>
				<ESDIRK>ESDIRK3</ESDIRK>				<!-- ESDIRK3 for four stages third order ESDIRK. ESDIRK4 for six stages fourth order ESDIRK. -->
			</ESDIRKLUSGS>
			<sourceTermTreating>explicitSource</sourceTermTreating>			<!-- explicitSource, diagImpliciSource or fullJacobian. Default is explicitSource. -->
		</timeMethod>

		<!--[1.6] Monitor. -->
		<monitor>
			<monitorList><![CDATA[residuals,point1, boundary1, force1, global]]></monitorList>
			<!--(1) Residuals. -->
			<residuals>
				<monitorType>residuals</monitorType>
				<updateStep>1</updateStep>
				<writeToFile>off</writeToFile>				<!-- On or off. Should write the information to the file. Default is off. -->
				<fileName>HurricaneResidual.dat</fileName>				<!-- Defalut is case name + "_residuals.dat".  -->
				<varList><![CDATA[rho,v,E,kt,wt]]></varList>				<!-- Notice: if vector-type variables, the vector name is only valid while the name of every component of this vecotr is not essential-->
				<residualConvergence>off</residualConvergence>				<!-- On or off, default is off. -->
				<rho>1e-6</rho>
				<v>(1e-6,1e-6,1e-6)</v>				<!-- Notice: if vector-type variables-->
				<E>1e-6</E>
				<kt>1e-6</kt>
				<wt>1e-6</wt>
			</residuals>

			<!--(2) Point. -->
			<point1>
				<monitorType>pointMonitors</monitorType>
				<updateStep>1</updateStep>
				<printToScreen>on</printToScreen>				<!-- On or off. Should print the information to the screen. Default is on. -->
				<writeToFile>off</writeToFile>				<!-- On or off. Should write the information to the file. Default is off. -->
				<fileName>point1.dat</fileName>				<!-- Defalut is case name + "_point1.dat".  -->
				<position>(0.0, 0.0, 0.0)</position>
				<varList><![CDATA[p,T,rho,u,v,w,E,kt,wt]]></varList>
			</point1>

			<!--(3) Boundary face zone. -->
			<boundary1>
				<monitorType>faceMonitors</monitorType>
				<updateStep>1</updateStep>
				<printToScreen>on</printToScreen>				<!-- On or off. Should print the information to the screen. Default is on. -->
				<writeToFile>off</writeToFile>				<!-- On or off. Should write the information to the file. Default is off. -->
				<fileName>boundary1.dat</fileName>				<!-- Defalut is case name + "_boundary1.dat".  -->
				<options>
					<perFaceZone>on</perFaceZone>					<!-- on or off. Default is off. -->
				</options>

				<!-- Valid report type: -->
				<!--	area, areaWeightedAverage, areaWeightedUniformityIndex, 
						facetAverage, facetMaximum, facetMinimum,
						flowRate, integral, massFlowRate,
						massWeightedAverage, massWeightedUniformityIndex, standardDeviation,
						sumOfFieldVariable, volumeFlowRate
				-->
				<reportType>massFlowRate</reportType>
				<zoneList><![CDATA[boundaryFace1]]></zoneList>				<!--Name list of boundary face zone. -->
				<var>rho</var>
			</boundary1>

			<!--(4) Force. -->
			<force1>
				<monitorType>forceMonitors</monitorType>
				<updateStep>1</updateStep>
				<printToScreen>on</printToScreen>				<!-- On or off. Should print the information to the screen. Default is on. -->
				<writeToFile>off</writeToFile>				<!-- On or off. Should write the information to the file. Default is off. -->
				<fileName>force1.dat</fileName>				<!-- Defalut is case name + "_force1.dat".  -->
				<options>
					<perFaceZone>on</perFaceZone>					<!-- on or off. Default is off. -->
					<reportPressureComponent>off</reportPressureComponent>					<!-- on or off. Default is off. -->
					<reportViscousComponent>off</reportViscousComponent>					<!-- on or off. Default is off. -->
					<reportCoordinateComponent>off</reportCoordinateComponent>					<!-- on or off. Default is off. -->
				</options>
				<zoneList><![CDATA[wallZoneName]]></zoneList>
				<reportType>force</reportType>				<!-- force or coefficient. -->
				<forceType>force</forceType>				<!-- force, drag, lift or moment. -->

				<!-- For forceType: force, drag and lift.-->
				<forceVector>(1,0,0)</forceVector>

				<!-- For forceType: moment. -->
				<momentCenter>(0,0,0)</momentCenter>
				<momentAxis>(0,0,1)</momentAxis>
			</force1>

			<!--(5) Drag. -->
			<drag1>
				<monitorType>forceMonitors</monitorType>
				<updateStep>1</updateStep>
				<printToScreen>on</printToScreen>				<!-- On or off. Should print the information to the screen. Default is on. -->
				<writeToFile>off</writeToFile>				<!-- On or off. Should write the information to the file. Default is off. -->
				<fileName>drag1.dat</fileName>				<!-- Defalut is case name + "_drag1.dat".  -->
				<options>
					<perFaceZone>on</perFaceZone>					<!-- on or off. Default is off. -->
					<reportPressureComponent>off</reportPressureComponent>					<!-- on or off. Default is off. -->
					<reportViscousComponent>off</reportViscousComponent>					<!-- on or off. Default is off. -->
					<reportCoordinateComponent>off</reportCoordinateComponent>					<!-- on or off. Default is off. -->
				</options>
				<zoneList><![CDATA[wallZoneName]]></zoneList>
				<reportType>force</reportType>				<!-- force or coefficient. -->
				<forceType>drag</forceType>				<!-- force, drag, lift or moment. -->
				<forceVector>(1,0,0)</forceVector>
			</drag1>

			<!--(6) Lift. -->
			<lift1>
				<monitorType>forceMonitors</monitorType>
				<updateStep>1</updateStep>
				<printToScreen>on</printToScreen>				<!-- On or off. Should print the information to the screen. Default is on. -->
				<writeToFile>off</writeToFile>				<!-- On or off. Should write the information to the file. Default is off. -->
				<fileName>lift1.dat</fileName>				<!-- Defalut is case name + "_lift1.dat".  -->
				<options>
					<perFaceZone>on</perFaceZone>					<!-- on or off. Default is off. -->
					<reportPressureComponent>off</reportPressureComponent>					<!-- on or off. Default is off. -->
					<reportViscousComponent>off</reportViscousComponent>					<!-- on or off. Default is off. -->
					<reportCoordinateComponent>off</reportCoordinateComponent>					<!-- on or off. Default is off. -->
					<reportPressureCenter>off</reportPressureCenter>					<!-- on or off. Default is off. -->
				</options>
				<zoneList><![CDATA[wallZoneName]]></zoneList>
				<reportType>force</reportType>				<!-- force or coefficient. -->
				<forceType>lift</forceType>				<!-- force, drag, lift or moment. -->
				<forceVector>(0,1,0)</forceVector>
			</lift1>

			<!--(7) Moment. -->
			<moment1>
				<monitorType>forceMonitors</monitorType>
				<updateStep>1</updateStep>
				<printToScreen>on</printToScreen>				<!-- On or off. Should print the information to the screen. Default is on. -->
				<writeToFile>off</writeToFile>				<!-- On or off. Should write the information to the file. Default is off. -->
				<fileName>moment1.dat</fileName>				<!-- Defalut is case name + "_moment1.dat".  -->
				<options>
					<perFaceZone>on</perFaceZone>					<!-- on or off. Default is off. -->
					<reportPressureComponent>off</reportPressureComponent>					<!-- on or off. Default is off. -->
					<reportViscousComponent>off</reportViscousComponent>					<!-- on or off. Default is off. -->
					<reportCoordinateComponent>off</reportCoordinateComponent>					<!-- on or off. Default is off. -->
				</options>
				<zoneList><![CDATA[wallZoneName]]></zoneList>
				<reportType>force</reportType>				<!-- force or coefficient. -->
				<forceType>moment</forceType>				<!-- force, drag, lift or moment. -->
				<momentCenter>(0,0,0)</momentCenter>
				<momentAxis>(0,0,1)</momentAxis>
			</moment1>
		</monitor>

	</iteration>

	<!--[2] Spatial Scheme-->
	<spatialScheme>
		<!--[2.1] Flux-->
			<spatialScheme>upwindSchemes</spatialScheme>		<!-- upwindSchemes, blendingSchemeTVDLimiter2, JST. -->
		
			<blendingSchemeTVDLimiter2>
				<upwindScheme>LDFSS2</upwindScheme>			<!--AUSMPWPlus, AUSMPlusUP, LDFSS2, HLLC, HLLC_HLL, AUSM, HLL, or vanLeer-->
				<minAlphaf>0.04</minAlphaf>			<!-- [0~1]. The allowed minimum factor of upwind part of the flux. Default is 0.1.-->
				<maxAlphaf>1</maxAlphaf>			<!-- [0~1]. The allowed maximum factor of upwind part of the flux. Default is 1.0. -->

				<temperatureSensorK>8e-5</temperatureSensorK>
				<THighLimitShock>100.0</THighLimitShock>			<!--Default is 100.-->
				<checkTemperature>on</checkTemperature>			<!--Default is on.-->
				<THighLimit>3000.0</THighLimit>			<!--Default is 3000.-->
				<TLowLimit>100.0</TLowLimit>			<!--Default is 100.-->
			</blendingSchemeTVDLimiter2>
			<upwindSchemes>
				<upwindScheme>HLLC</upwindScheme>			<!--AUSMPWPlus, AUSMPlusUP, LDFSS2, HLLC, HLLC_HLL, AUSM, HLL, or vanLeer-->

				<AUSMPlusUP>
					<!--AUSMPlusUP scheme parameters>-->
					<Kp>0.25</Kp>
					<Ku>0.75</Ku>
					<sigma>1.0</sigma>
					<beta>0.125</beta>
				</AUSMPlusUP>

				<HLLC>
					<lowMachCorrected>off</lowMachCorrected>				<!-- Default is off. -->
					<enhancedShockStability>off</enhancedShockStability>				<!-- Default is off. -->
				</HLLC>
			</upwindSchemes>

			<JST>
				<k2>0.5</k2>
				<k4>7.8125e-3</k4>			<!-- [1/128 ~ 1/64] -->
				<useModifyFactor>off</useModifyFactor>			<!-- Use a factor to modify pressure sensor to reduce numerical dissipation. Default is off. -->
			</JST>


		<!--[2.2] Reconstruction-->
		
			<!--First order-->
			<!--<reconstruction>firstOrder</reconstruction>-->

			<!--Multi-Dimensional Linear Method-->
			<reconstruction>linear</reconstruction>
			<limitersForLinear>Venk</limitersForLinear>		<!-- Barth or Venk-->
			<K>5.0</K>		<!-- Venk parameters. If Mach number is large, it is recommended to reduce K. -->
		
		<!--[2.3] Gradient-->
			<gradient>cellGaussGrad</gradient>		<!-- nodeGaussGrad, leastSquareGrad or cellGaussGrad-->

			<!--For "leastSquareGrad" -->
				<weightType>WLSQG</weightType>		<!-- Least Square Gradent Weight Type : WLSQ0, WLSQ1, WLSQ2, WLSQ3, WLSQG-->
				<cellNeighbourCell>FACENEIGHBOUR</cellNeighbourCell>		<!-- FACENEIGHBOUR, NODENEIGHBOUR or TWOFACENEIGHBOUR-->
	</spatialScheme>

	<!--[3] Flow Setting-->
	<flow>

		<!--[3.1] Flow Model-->
		<flowModel>eddyViscosity</flowModel>		<!-- EulerFlow, laminarFlow or eddyViscosity-->
		<Prl>0.72</Prl>		<!-- Laminar Prandtl number-->

		<!--[3.2] Turbulence Setting-->
		<Prt>0.90</Prt>		<!-- Turbulent Prandtl number-->
		<Sct>0.5</Sct>		<!-- Turbulent Schmidt number-->
		<turbulence>
			<distanceMethod>searchProcedures</distanceMethod>
			<solverType>coupled</solverType>			<!-- coupled or splitting-->
			<turbulenceModel>SST</turbulenceModel>			<!-- SpalartAllmaras, SST-->
			<coefTurb>0.05</coefTurb>			<!-- The limit coefficient for RANS turb parameter increase rate. Default is 0.05. -->			
			<SST>
				<SSTVersion>SST-2003</SSTVersion>				<!-- standardSST, SST-2003 or SST-sust-->
				<a1>0.31</a1>				<!-- Default 0.31   -->
				<a2>0.15</a2>				<!-- Default 0.15   -->
				<a3>0.20</a3>				<!-- Default 0.20   -->
				<betas>0.091</betas>		<!-- Default 0.091  -->
				<sigmak1>0.85</sigmak1>		<!-- Default 0.85   -->
				<sigmak2>1.0</sigmak2>		<!-- Default 1.0    -->
				<sigmaw1>0.50</sigmaw1>		<!-- Default 0.50   -->
				<sigmaw2>0.856</sigmaw2>	<!-- Default 0.856  -->
				<beta1>0.075</beta1>		<!-- Default 0.075  -->
				<beta2>0.0828</beta2>		<!-- Default 0.0828 -->
				<kRelax>1.0</kRelax>		<!-- Default 1.0    -->
				<wRelax>1.0</wRelax>		<!-- Default 1.0    -->
			</SST>

			<SpalartAllmaras>
				<SAVersion>SA</SAVersion>	<!-- SA or SANEG     -->
				<cb1>0.1355</cb1>			<!-- Default 0.1355  -->
				<cb2>0.622</cb2>			<!-- Default 0.622   -->
				<Sigma>0.66667</Sigma>		<!-- Default 0.66667 -->
				<cv1>7.1</cv1>				<!-- Default 7.1     -->
				<cv2>0.7</cv2>				<!-- Default 0.7     -->
				<cv3>0.9</cv3>				<!-- Default 0.9     -->
				<kappa>0.41</kappa>			<!-- Default 0.41    -->
				<cw2>0.3</cw2>				<!-- Default 0.3     -->
				<cw3>2.0</cw3>				<!-- Default 2.0     -->
				<ct3>1.2</ct3>				<!-- Default 0.3     -->
				<ct4>0.5</ct4>				<!-- Default 1.2     -->
				<nutRelax>1.0</nutRelax>	<!-- Default 0.5     -->
			</SpalartAllmaras>
		</turbulence>

		<!--[3.3] Limits Setting-->
		<limits>
			<limitMethod>directly</limitMethod>			<!-- directly or average-->
			<!--Temperature-->
				<THigh>3800.0</THigh>
				<TLow>110.0</TLow>
			<!--Pressure-->
				<pHigh>6930000</pHigh>
				<pLow>1.0</pLow>
			<!--Eddy Viscousity-->
				<mutHigh>2.0e5</mutHigh>
				<mutLow>1.0e-15</mutLow>

			<minKt>1e-14</minKt>			<!-- default is 1e-14 -->
